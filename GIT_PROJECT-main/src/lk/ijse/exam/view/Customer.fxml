<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.exam.controller.CustomerController">
   <children>
      <TextField fx:id="customerId" layoutX="202.0" layoutY="93.0" promptText="Customer Id">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Label layoutX="25.0" layoutY="97.0" text="Customer Id :">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="customerName" layoutX="202.0" layoutY="143.0" promptText="Customer Name">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Label layoutX="25.0" layoutY="147.0" text="Customer Name :">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="customerAddress" layoutX="202.0" layoutY="190.0" promptText="Customer Address">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Label layoutX="25.0" layoutY="194.0" text="Customer Address :">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button layoutX="242.0" layoutY="288.0" mnemonicParsing="false" onAction="#onBtnAddCustomerClick" text="Add Customer">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label layoutX="177.0" layoutY="34.0" text="Customer Registration">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button layoutX="537.0" layoutY="21.0" mnemonicParsing="false" onAction="#onBtnHomeClick" text="Home" />
   </children>
</AnchorPane>
